{
  "name": "remix-example-turborepo-vercel",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "clean": "git clean -fdx -e .vercel .",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write .",
    "lint": "turbo run lint",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "patch-package": "6.4.7",
    "postinstall-postinstall": "2.1.0",
    "prettier": "^2.5.1",
    "turbo": "^1.2.5"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  }
}
